{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions":["activeTab","storage"],
  "host_permissions":[
    "https://localhost:9000/*",
    "://mail.google.com/*"
  ],
  "action": {
    "default_popup": "hello.html",
    "default_icon": "icon.png"
  }
}